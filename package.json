{
  "name": "paratii-mediaplayer",
  "version": "1.0.2",
  "description": "Clappr-based video player for Paratii",
  "main": "build/bundle.js",
  "scripts": {
    "dev": "yarn run clean && yarn run prep && NODE_ENV=dev webpack-dev-server -d --content-base ./build --hot --progress --colors --config webpack.demo.js",
    "build": "yarn run clean && yarn run prep && NODE_ENV=production webpack --progress --colors",
    "build:watch": "yarn run build -- --watch",
    "prep": "mkdir build/ && cp src/index.html build/ && mkdir build/assets && cp -r src/assets build/",
    "clean": "rm -rf build/",
    "prepublishOnly": "yarn run build",
    "lint": "eslint src test --ext .js --cache",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache",
      "prettier --write",
      "git add"
    ]
  },
  "author": "Yahya",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clappr": "0.2.78",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "html-loader": "^0.5.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "node-sass": "^4.7.2",
    "open-browser-webpack-plugin": "^0.0.5",
    "prettier": "^1.9.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-transition-group": "^2.2.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.5"
  },
  "dependencies": {
    "hlsjs-ipfs-loader": "github:ya7ya/hlsjs-ipfs-loader#paratii/v0.27.1",
    "ipfs": "github:ya7ya/js-ipfs#paratii/v0.27.6",
    "ipfs-bitswap": "^0.17.4",
    "resolve-url-loader": "^2.2.1",
    "svg-url-loader": "^2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Paratii-Video/paratii-mediaplayer.git"
  },
  "bugs": {
    "url": "https://github.com/Paratii-Video/paratii-mediaplayer/issues"
  },
  "homepage": "https://github.com/Paratii-Video/paratii-mediaplayer#readme",
  "peerDependencies": {
    "clappr": "^0.2.78",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-transition-group": "^2.2.1"
  }
}
